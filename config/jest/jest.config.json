{
  "rootDir": "../../",
  "testMatch": ["**/__tests__/**/*.(spec|test).js?(x)"],
  "setupFilesAfterEnv": ["<rootDir>/config/jest/setupTests.js"],
  "moduleNameMapper": {
    "\\.(css|sass|scss)$": "identity-obj-proxy",
    "~/(.*)": "<rootDir>/src/$1"
  },
  "verbose": true,
  "collectCoverage": false,
  "collectCoverageFrom": [
    "<rootDir>/src/**/*.js"
  ],
  "coverageReporters": ["text", "html"]
}