    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.6.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-root-import": "^6.4.1",
    "babel-plugin-transform-imports": "^1.5.1",
    "body-parser": "^1.19.0",
    "clean-webpack-plugin": "^3.0.0",
    "compression": "^1.7.4",
